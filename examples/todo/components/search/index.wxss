.search {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    padding: 0 16rpx;
    padding-top: env(safe-area-inset-top);
    padding-bottom: env(safe-area-inset-bottom);
    background-color: #778591;
    z-index: 999;
    color: #FFFFFF;
}

.search-handler {
    width: 100%;
    height: 80rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.icon-wrapper {
    width: 80rpx;
    height: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.icon-wrapper image {
    width: 60rpx;
    height: 60rpx;
}

.search-handler input {
    flex-grow: 1;
}

.cancel {
    width: 100rpx;
    text-align: center;
}

.guide{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 200rpx;
}

.guide image {
    width: 400rpx;
    height: 400rpx;
}
